<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="cs.laneskaintza">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main">
 	<h1 class="documentFirstHeading">
            <metal:field use-macro="python:here.widget('title', mode='view')">
            Title
            </metal:field>
  	</h1>
  <div class="laneskaintzaview">


  <dl>

	<tal:block tal:define="information python: here.getInformation()" tal:condition="information">
	<dt i18n:translate="label_laneskaintza_information"></dt>
	<dd tal:define="information python:here.getInformation()" tal:content="structure information"></dd>
	</tal:block>

  <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['temario', 'modelo_instancia']]">

      <tal:if_visible define="mode string:view;
                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data;"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>
    <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['file_information']]">

      <tal:if_visible define="mode string:view;
                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data;"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>

	<tal:block tal:define="data python: here.getStart_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_startdata"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
	</tal:block>

        <tal:block tal:define="data python: here.getEnd_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_enddata"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data, long_format=1)" tal:content="data"></dd>
	</tal:block>

       <tal:block tal:define="data python: here.getPrensa_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_prensadata"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
	</tal:block>

        <tal:block tal:define="data python: here.getStart_data_bog()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_startdata_bog"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
	</tal:block>

        <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['file_prensa']]">

      <tal:if_visible define="mode string:view;
                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data;"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>

	<tal:block tal:define="contact_information python: here.aq_parent.getContact_information()" tal:condition="contact_information">
	<dt i18n:translate="label_laneskaintza_contact_information"></dt>
	<dd tal:define="contact_information python:here.getContact_information()" tal:content="structure contact_information"></dd>
	</tal:block>

  <tal:block tal:define="data python: here.getFile_enrolled_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_file_enrolled_data"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
  </tal:block>
   <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['file_enrolled']]">

      <tal:if_visible define="mode string:view;
                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data;"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>
   <tal:block tal:define="data python: here.getFinal_file_enrolled_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_final_file_enrolled_data"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
  </tal:block>
   <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['final_file_enrolled']]">

      <tal:if_visible define="mode string:view;
                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data;"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>

    <tal:block tal:define="information python: here.getInformation_examenes()" tal:condition="information">
	<dt i18n:translate="label_laneskaintza_information_examenes"></dt>
	<dd tal:define="information python:here.getInformation_examenes()" tal:content="structure information"></dd>
	</tal:block>

   <tal:block tal:define="data python: here.getFile_exercise_results_data()" tal:condition="data">
	<dt i18n:translate="label_laneskaintza_file_exercise_results_data"></dt>
	<dd tal:define=" data python: here.toLocalizedTime(data)" tal:content="data"></dd>
  </tal:block>

   <tal:files
        define="field_macro field_macro | here/widgets/field/macros/base_view"
        repeat="field python:[here.getField(f) for f in ['file_exercise_results']]">

      <tal:if_visible define="mode string:view;

                              accessor nocall:field/accessor;
                              visState python:field.widget.isVisible(here, mode);
                              visCondition python:field.get_size(here);
                              label_macro here/laneskaintza_general/macros/label;
                              data_macro here/laneskaintza_general/macros/data"
                      condition="python:visState == 'visible' and visCondition">
        <metal:use_field use-macro="field_macro" />
      </tal:if_visible>
    </tal:files>
    </dl>

    </div>
        </tal:main-macro>

    </div>
</body>
</html>
