<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<body>

    <div metal:fill-slot="main">
    <metal:main-macro define-macro="main">

        <div metal:use-macro="here/document_actions/macros/document_actions">
            Document actions (print, sendto etc)
        </div>
	<h1 class="documentFirstHeading"> 
          <tal:content content="here/Title"></tal:content>
	</h1>
	<ul id="tabs">
	<li><a tal:attributes="href string:${here/absolute_url}" i18n:domain="LanEskaintza" i18n:translate="zabalik">Alta emateko zabalik daudenak</a></li>
	<li><a tal:attributes="href string:${here/absolute_url}/prozesuan" i18n:domain="LanEskaintza" i18n:translate="prozesuan">Prozesuan</a></li>
        <li class="on"><a tal:attributes="href string:${here/absolute_url}/past" i18n:domain="LanEskaintza" i18n:translate="bukatutakoak">Bukatutakoak</a></li>
	</ul>
       <metal:listingmacro define-macro="listing">
        <tal:foldercontents define="bista here/laneskaintza_lista;edukia python: bista.laneskaintza_lista(3); batch edukia">
         <div id="laneskaintzak">
         <tal:listing repeat="laneskaintza batch">
	
	<div class="laneskaintza">
	<h2 class="tileHeadline"><a tal:attributes="href laneskaintza/getURL" tal:content="laneskaintza/Title"></a></h2>
         </div>
        
        </tal:listing>
	</div>
        <div metal:use-macro="here/batch_macros/macros/navigation" />
         
        </tal:foldercontents>
        
        
        </metal:listingmacro>
    </metal:main-macro>
    </div>

</body>
</html>
